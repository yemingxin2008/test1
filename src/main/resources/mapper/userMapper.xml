<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ymx.mapper.UserMapper">

    <insert id="saveUser" parameterType="com.ymx.entity.User">
        INSERT INTO users(name,age,create_time)   VALUES
        ( #{name},#{age},now())
    </insert>


</mapper>
